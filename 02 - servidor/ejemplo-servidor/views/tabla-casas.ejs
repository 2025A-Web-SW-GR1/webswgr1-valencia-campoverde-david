<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Tabla de Casas</title>
</head>
<body>
    <h1>Lista de Casas</h1>
    <table>
        <thead>
            <tr>
                <th>ID</th>
                <th>Nombre</th>
                <th>Valor</th>
                <th>Imagen</th>
                <th>Propietario</th>
            </tr>
        </thead>
        <tbody>
            <% casas.forEach(casa => { %>
                <tr>
                    <td><%= casa.id %></td>
                    <td><%= casa.nombre %></td>
                    <td><%= casa.valor %></td>
                    <td><img src="<%= casa.imagenURL %>" alt="<%= casa.nombre %>" /></td>
                </tr>
            <% }) %>
        </tbody>
    </table>
</body>
</html>